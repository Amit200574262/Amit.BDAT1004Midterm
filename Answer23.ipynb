    "def tough(indent, num):\n",
    "    if(num > 0):\n",
    "        tough(indent, num//2)         \n",
    "        print(\" \"*indent + \"*\"*num);    \n",
    "        tough(indent + 1, num//2)\n",
    "        \n",
    "tough(0, 0)\n",
    "print(\"--------\")\n",
    "tough(0, 1)\n",
    "print(\"---------\")\n",
    "tough(0, 2)\n",
    "print(\"---------\")\n",
    "tough(0,4)"
